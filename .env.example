# Copy this file to .env and fill in your values
# Do NOT commit .env file to git

# MongoDB Atlas Connection String
# If your password contains special characters (for example '@'), URL-encode them
# Example: password 'MyPass@123' becomes 'MyPass%40123'
MONGODB_URI=mongodb+srv://your-username:your-password@cluster.xxxxx.mongodb.net/BhagirthiWoolCrafts

# JWT Secret (use a long random string)
JWT_SECRET=your-super-secret-jwt-key-change-this

# Session Secret
SESSION_SECRET=your-session-secret-key-change-this

# Port (Render will set this automatically)
PORT=3000
